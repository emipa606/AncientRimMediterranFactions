<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <PawnKindDef Name="PawnRomeBase" Abstract="True">
    <!-- <defaultFactionType>AR_Faction_Rome</defaultFactionType> -->
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>3~6</initialResistanceRange>
    <!--
    <backstoryCategories MayRequire="shavius.classical.flavour">
		<li MayRequire="shavius.classical.flavour">Classical</li>
	</backstoryCategories>
	-->
    <backstoryFiltersOverride MayRequire="shavius.classical.flavour">
      <li MayRequire="shavius.classical.flavour">
        <categoriesChildhood MayRequire="shavius.classical.flavour">
          <li MayRequire="shavius.classical.flavour">Classical</li>
        </categoriesChildhood>
      </li>
    </backstoryFiltersOverride>
	<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
	<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <apparelTags>
      <li>RomeCivil</li>
    </apparelTags>
    <invFoodDef>AR_Salad</invFoodDef>
    <inventoryOptions>
      <skipChance>0.3</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>33</choiceChance>
          <thingDef>AR_RawOlives</thingDef>
          <countRange>
            <min>15</min>
            <max>55</max>
          </countRange>
        </li>
        <li>
          <choiceChance>35</choiceChance>
          <thingDef>AR_OliveOil</thingDef>
          <countRange>
            <min>15</min>
            <max>40</max>
          </countRange>
        </li>
        <li>
          <choiceChance>25</choiceChance>
          <thingDef>AR_RawArtichoke</thingDef>
          <countRange>
            <min>15</min>
            <max>40</max>
          </countRange>
        </li>
        <li>
          <choiceChance>33</choiceChance>
          <thingDef>AR_RawPomegranate</thingDef>
          <countRange>
            <min>15</min>
            <max>40</max>
          </countRange>
        </li>
        <li MayRequire="oskarpotocki.vfe.classical">
          <choiceChance MayRequire="oskarpotocki.vfe.classical">20</choiceChance>
          <thingDef MayRequire="oskarpotocki.vfe.classical">VFEC_Jerky</thingDef>
          <countRange MayRequire="oskarpotocki.vfe.classical">
            <min MayRequire="oskarpotocki.vfe.classical">20</min>
            <max MayRequire="oskarpotocki.vfe.classical">25</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
  <PawnKindDef Name="RomeCitizen" ParentName="PawnRomeBase">
    <defName>AR_Citizen</defName>
    <label>civitas</label>
    <labelPlural>civitates</labelPlural>
    <fixedGender>Male</fixedGender>
    <!-- Separating females from males so that only men wear togas, which was a thing since 509 BC in the Roman Republic and beyond -->
    <race>Human</race>
    <combatPower>30</combatPower>
    <itemQuality>Normal</itemQuality>
    <isFighter>false</isFighter>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>5~14</initialResistanceRange>
	<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <backstoryFiltersOverride MayRequire="shavius.classical.flavour">
      <li MayRequire="savius.classical.flavour">
        <categoriesAdulthood>
          <li MayRequire="shavius.classical.flavour">Classical</li>
        </categoriesAdulthood>
      </li>
    </backstoryFiltersOverride>
    <gearHealthRange>
      <min>0.8</min>
      <max>2.0</max>
    </gearHealthRange>
    <apparelMoney>
      <min>300</min>
      <max>500</max>
    </apparelMoney>
    <apparelAllowHeadgearChance>0.2</apparelAllowHeadgearChance>
    <weaponMoney>
      <min>80</min>
      <max>500</max>
    </weaponMoney>
    <weaponTags>
      <li>Toolagri</li>
      <li>NeolithicMelee</li>
      <li>NeolithicMeleeBasic</li>
      <li>Knife</li>
    </weaponTags>
    <techHediffsMoney>
      <min>200</min>
      <max>300</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.001</techHediffsChance>
    <invNutrition>3</invNutrition>
    <apparelTags>
      <li>RomeCivil</li>
    </apparelTags>
    <!-- ALL Roman citizens wore togas, the toga was LITERALLY the symbol of citizenship -->
    <apparelRequired>
      <li>AR_Toga</li>
    </apparelRequired>
  </PawnKindDef>
  <PawnKindDef Name="RomeSocialiteGirl" ParentName="PawnRomeBase">
    <defName>TSP_AR_CitizenGirl</defName>
    <label>plebian</label>
    <labelPlural>plebes</labelPlural>
    <fixedGender>Female</fixedGender>
    <!-- Separating females from males so that only men wear togas, which was a sex-segregated thing since 509 BC in the Roman Republic and beyond -->
    <race>Human</race>
    <combatPower>5</combatPower>
    <itemQuality>Poor</itemQuality>
    <isFighter>false</isFighter>
    <canBeSapper>true</canBeSapper>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>1~7</initialResistanceRange>
	<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <backstoryFiltersOverride MayRequire="shavius.classical.flavour">
      <li MayRequire="savius.classical.flavour">
        <categoriesAdulthood>
          <li MayRequire="shavius.classical.flavour">Classical</li>
        </categoriesAdulthood>
      </li>
    </backstoryFiltersOverride>
    <requiredWorkTags>
      <li>Social</li>
    </requiredWorkTags>
    <gearHealthRange>
      <min>0.5</min>
      <max>0.9</max>
    </gearHealthRange>
    <apparelMoney>
      <min>700</min>
      <max>2500</max>
      <!-- Roman women tended to be artisans -->
    </apparelMoney>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <weaponMoney>
      <min>80</min>
      <max>500</max>
    </weaponMoney>
    <weaponTags>
      <li>Toolagri</li>
      <!-- <li>Neolithic</li> -->
      <li>NeolithicMelee</li>
      <li>NeolithicMeleeBasic</li>
      <li>NeolithicMeleeDestructive</li>
    </weaponTags>
    <techHediffsMoney>
      <min>200</min>
      <max>300</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.01</techHediffsChance>
    <invNutrition>3</invNutrition>
    <apparelTags>
      <li>RomeCivil</li>
      <!-- <li MayRequire="ludeon.rimworld.royalty">Cape</li> -->
      <!-- Doesn't need the tag because it's already covered below. This will stop two capes from spawning on one Pawn. -->
    </apparelTags>
    <apparelRequired>
      <li MayRequire="ludeon.rimworld.royalty">Apparel_Cape</li>
      <!-- Closest thing to the stola, which was the woman's equivalent of a toga -->
      <li>AR_Tunic</li>
    </apparelRequired>
  </PawnKindDef>
  <PawnKindDef Name="RomePatrician" ParentName="RomeCitizen">
    <defName>AR_Councilman</defName>
    <label>Senator</label>
    <labelFemale>Senatrix</labelFemale>
    <labelPlural>Senatores</labelPlural>
    <minGenerationAge>40</minGenerationAge>
    <maxGenerationAge>100</maxGenerationAge>
    <factionLeader>true</factionLeader>
    <isFighter>false</isFighter>
	<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
	<favoriteColor>(99,3,48)</favoriteColor>
    <!-- <fixedGender>Male</fixedGender> -->
    <!-- They may complain about wearing a men's garment, and this is intended, but Senators on the Rim shouldn't neccessarily be all male -->
    <backstoryFiltersOverride>
      <li>
        <categoriesAdulthood>
          <li>ImperialCommon</li>
          <li MayRequire="shavius.classical.flavour">Classical</li>
        </categoriesAdulthood>
      </li>
    </backstoryFiltersOverride>
    <requiredWorkTags Inherit="False">
      <li>Social</li>
    </requiredWorkTags>
    <gearHealthRange>
      <min>1.0</min>
      <max>3.0</max>
    </gearHealthRange>
    <apparelMoney>
      <min>900</min>
      <max>1800</max>
    </apparelMoney>
	<weaponTags>
		<li>ClassicalTrader</li> <!-- Inherits from the Base -->
	</weaponTags>
    <apparelTags>
      <li>RomeCivil</li>
      <li>RomeNoble</li>
    </apparelTags>
    <apparelRequired Inherit="False">
      <!-- Roman Senators did not wear laurel wreaths -->
      <li>AR_Toga</li>
    </apparelRequired>
  </PawnKindDef>
